class dog
{
	unprotected int factorial(int x)
	{
		int temp;
	
		if (x <= 1)
			return 1;
			
		temp = x * factorial(x - 1);
		return temp;
	}
}

	
void kxi2015 main()
{
	int x = 5;
	char w;
	dog d;
	
	cout << 'x';
	cout << '1';
	cout << ':';
	cout << ' ';
	cout << x;
	cout << '\n';
	
	x = d.factorial(x);
	
	cout << 'x';
	cout << '2';
	cout << ':';
	cout << ' ';
	cout << x;
	cout << '\n';
}
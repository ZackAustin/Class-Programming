class Dog
{
	unprotected char letter = 'y';
	Dog(char c)
	{
		letter = c;
	}
}

void kxi2015 main()
{
	int x = 0;
	int arraySize = 5;
	Dog someDogs[] = new Dog[arraySize];
	
	someDogs[0] = new Dog('Z');
	someDogs[1] = new Dog('a');
	someDogs[2] = new Dog('c');
	someDogs[3] = new Dog('k');
	someDogs[4] = new Dog('!');
	
	while (x < arraySize)
	{
		cout << x;
		cout << ':';
		cout << ' ';
		
		cout << someDogs[x].letter;
		
		cout << '\n';
		x = x + 1;
	}
}